{
  "name": "directus-auth-manager",
  "version": "1.0.1",
  "description": "CLI tool and library for managing multiple Directus credential sets",
  "type": "module",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexvdvalk/directus-auth-manager.git"
  },
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "bin": {
    "directus-auth": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "directus",
    "cli",
    "auth",
    "credentials",
    "manager"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
